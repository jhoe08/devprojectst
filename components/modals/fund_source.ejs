    <select id="fsPAP" class="form-select form-select-sm"><option value="">-- select PAP --</option></select>
  </div>
  <div class="col-md-3">
    <label class="form-label small">CLASS</label>
    <select id="fsCLASS" class="form-select form-select-sm"><option value="">-- select CLASS --</option></select>
  </div>
  <div class="col-md-3">
    <label class="form-label small">OBJ CODE</label>
    <select id="fsOBJ" class="form-select form-select-sm"><option value="">-- select OBJ --</option></select>
  </div>
  <div class="col-md-2">
    <label class="form-label small">SOURCE</label>
    <select id="fsSource" class="form-select form-select-sm" style="display:none"><option value="">-- select Source --</option></select>
  </div>
  <div class="col-12 mt-2">
    <label class="form-label small">Description</label>
    <select id="fsDescription" class="form-select form-select-sm"><option value="">-- select Description --</option></select>
  </div>
</div>
<div class="mt-3"><div id="fsResult" class="small text-muted"></div></div>

<script>
// Expose the array assigned above as a window property for UI code below
window.FUND_SOURCE_DATA = (typeof FUND_SOURCE_DATA !== 'undefined') ? FUND_SOURCE_DATA : (window.FUND_SOURCE_DATA || []);

// Helper UI container: insert a small panel with selects for Mode, PAP, CLASS, OBJ, DESCRIPTION, SOURCE
(function(){

  // Utilities
  const data = (window.FUND_SOURCE_DATA || []).slice(1); // skip header row

  function isContinuingRow(row) {
    if (!row || typeof row[5] === 'undefined') return false;
    return String(row[5]).toLowerCase().includes('continuing');
  }

  function cleanRow(row) {
    // Skip totals and empty rows
    if (!row || !row[0]) return null;
    const first = String(row[0]||'');
    if (first.toLowerCase().includes('total') || first.trim() === '' || first.toLowerCase().includes('grand total')) return null;
    return row;
  }

  function getDataset(mode) {
    const rows = data.map(cleanRow).filter(Boolean);
    if (mode === 'continuing') return rows.filter(isContinuingRow);
    return rows.filter(r => !isContinuingRow(r));
  }

  // Distinct helper for a specific index
  function distinctByIndex(rows, idx) {
    const s = new Set();
    const out = [];
    rows.forEach(r => {
      const val = (r[idx] || '').toString();
      if (!s.has(val)) { s.add(val); out.push(val); }
    });
    return out;
  }

  // DOM refs
  const btnCurrent = document.getElementById('fsModeCurrent');
  const btnContinuing = document.getElementById('fsModeContinuing');
  const selPAP = document.getElementById('fsPAP');
  const selCLASS = document.getElementById('fsCLASS');
  const selOBJ = document.getElementById('fsOBJ');
  const selSource = document.getElementById('fsSource');
  const selDesc = document.getElementById('fsDescription');
  const result = document.getElementById('fsResult');

  let currentMode = 'current';

  function setMode(m) {
    currentMode = m;
    btnCurrent.classList.toggle('btn-primary', m === 'current');
    btnCurrent.classList.toggle('btn-outline-primary', m !== 'current');
    btnContinuing.classList.toggle('btn-primary', m === 'continuing');
    btnContinuing.classList.toggle('btn-outline-primary', m !== 'continuing');
    // show source select only for continuing
    selSource.style.display = (m === 'continuing') ? '' : 'none';
    rebuildPAP();
  }

  function rebuildPAP() {
    const rows = getDataset(currentMode);
    const paps = distinctByIndex(rows, 1).filter(v => v && v.trim() !== '');
    selPAP.innerHTML = '<option value="">-- select PAP --</option>' + paps.map(p => `<option value="${p}">${p}</option>`).join('');
    selCLASS.innerHTML = '<option value="">-- select CLASS --</option>';
    selOBJ.innerHTML = '<option value="">-- select OBJ --</option>';
    selDesc.innerHTML = '<option value="">-- select Description --</option>';
    selSource.innerHTML = '<option value="">-- select Source --</option>';
    result.textContent = `Dataset: ${currentMode}  ${rows.length} rows`;
  }

  function onPAPChange() {
    const pap = selPAP.value;
    const rows = getDataset(currentMode).filter(r => (r[1]||'') === pap);
    const classes = distinctByIndex(rows, 2).filter(v => v && v.trim() !== '');
    selCLASS.innerHTML = '<option value="">-- select CLASS --</option>' + classes.map(c => `<option value="${c}">${c}</option>`).join('');
    // clear downstream
    selOBJ.innerHTML = '<option value="">-- select OBJ --</option>';
    selDesc.innerHTML = '<option value="">-- select Description --</option>';
    // populate source (if continuing mode)
    if (currentMode === 'continuing') {
      const sources = distinctByIndex(rows, 5).filter(v => v && v.trim() !== '');
      selSource.innerHTML = '<option value="">-- select Source --</option>' + sources.map(s => `<option value="${s}">${s}</option>`).join('');
    }
    result.textContent = `PAP '${pap}'  ${rows.length} matching rows`;
  }

  function onCLASSChange() {
    const pap = selPAP.value; const cls = selCLASS.value;
    let rows = getDataset(currentMode).filter(r => (r[1]||'') === pap && (r[2]||'') === cls);
    const objs = distinctByIndex(rows, 3).filter(v => v && v.trim() !== '');
    selOBJ.innerHTML = '<option value="">-- select OBJ --</option>' + objs.map(o => `<option value="${o}">${o}</option>`).join('');
    const descs = distinctByIndex(rows, 4).filter(v => v && v.trim() !== '');
    selDesc.innerHTML = '<option value="">-- select Description --</option>' + descs.map(d => `<option value="${d}">${d}</option>`).join('');
    result.textContent = `CLASS '${cls}'  ${rows.length} rows`;
  }

  function onOBJChange() {
    const pap = selPAP.value; const cls = selCLASS.value; const obj = selOBJ.value;
    const rows = getDataset(currentMode).filter(r => (r[1]||'') === pap && (r[2]||'') === cls && (r[3]||'') === obj);
    const descs = distinctByIndex(rows, 4).filter(v => v && v.trim() !== '');
    selDesc.innerHTML = '<option value="">-- select Description --</option>' + descs.map(d => `<option value="${d}">${d}</option>`).join('');
    result.textContent = `OBJ '${obj}'  ${rows.length} rows`;
  }

  // When description selected, show full row in result
  function onDescChange() {
    const pap = selPAP.value; const cls = selCLASS.value; const obj = selOBJ.value; const desc = selDesc.value;
    const rows = getDataset(currentMode).filter(r => (r[1]||'') === pap && (r[2]||'') === cls && (r[3]||'') === obj && (r[4]||'') === desc);
    if (rows.length) {
      const r = rows[0];       
      result.innerHTML = `<pre style="white-space:pre-wrap">${JSON.stringify({FUND:r[0],PAP:r[1],CLASS:r[2],OBJ:r[3],DESCRIPTION:r[4],SOURCE:r[5],ADJUSTED:r[6],OBLIGATION:r[7],UNOBLIGATED:r[8],EARMARK:r[9]},null,2)}</pre>`;
    } else {
      result.textContent = 'No matching row.';
    }
  }

  // Event bindings
  btnCurrent.addEventListener('click', () => setMode('current'));
  btnContinuing.addEventListener('click', () => setMode('continuing'));
  selPAP.addEventListener('change', onPAPChange);
  selCLASS.addEventListener('change', onCLASSChange);
  selOBJ.addEventListener('change', onOBJChange);
  selDesc.addEventListener('change', onDescChange);

  // initialize
  setMode('current');

})();
</script>
